page {
    width: 100%;
    height: 100%;
}

.cartion {
    width: 100%;
    height: 300rpx;
    margin-top: 50rpx;
    position: relative;
}

.music_bei {
    width: 100%;
    height: 100%;
    position: absolute;
    top: 0;
    left: 0;
}

.changp {
    width: 360rpx;
    height: 360rpx;
    position: absolute;
    top: 20rpx;
    left: 26%;
    z-index: 2;
}

.rotate-img {
    animation: rotate 10s linear infinite;
}

@keyframes rotate {
    form {
        transform: rotate(0deg);
    }

    to {
        transform: rotate(360deg);
    }
}

.zj02 {
    width: 218rpx;
    height: 300rpx;
    position: absolute;
    right: 0;
    z-index: 2;
    transform-origin: 20% 40%;
}