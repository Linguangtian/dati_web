page {
    background: #ffe9c2;
    box-sizing: border-box;
    padding: 20rpx;
}

.more_sufn {
    display: flex;
    justify-content: center;
    flex-direction: column;
    margin-bottom: 20rpx;
}

.more_sufn image {
    width: 100%;
    height: 300rpx;
}

.more_gongis {
    width: 100%;
    box-sizing: border-box;
    display: flex;
    background: #fff1da;
    padding-left: 20rpx;
    align-items: center;
    justify-content: space-between;
    padding: 10rpx;
}

.more_gongis text {
    color: #333333;
    font-size: 30rpx;
}

.more_gongis .more_yuas {
    padding: 0 25rpx;
    border-radius: 50rpx;
    color: #666666;
    font-size: 24rpx;
}

.digasop {
    width: 50%;
    text-align: left;
    display: flex;
    flex-direction: column;
    color: #666666;
    font-size: 24rpx;
    word-wrap: break-word;
}

.more_gongis .digasop text {
    color: #333333;
    font-size: 24rpx;
    height: 60rpx;
    vertical-align: middle;
    padding: 0;
}

.qieh {
    width: 100%;
    display: flex;
    align-items: center;
    justify-content: center;
    height: 80rpx;
    border: 2rpx solid #cb543e;
    margin-bottom: 20rpx;
    flex-direction: row;
}

.qieh text {
    width: 50%;
    display: flex;
    align-items: center;
    justify-content: center;
    color: #cb543e;
    height: 100%;
    font-size: 28rpx;
}

.qieh .tes {
    background: #cb543e;
    color: #fee6b1;
}